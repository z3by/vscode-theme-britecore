  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#6272a4",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#efb38f"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#efb38f"
      }
    },
    {
      "name": "Built-in constant and built-in variable",
      "scope": "constant.language, variable.language",
      "settings": {
        "foreground": "#efb38f"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#efb38f"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Ruby's @variable",
      "scope": "variable.other.readwrite.instance",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffb86c"
